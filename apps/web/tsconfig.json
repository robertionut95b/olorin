{
  "extends": "./.svelte-kit/tsconfig.json",
  "include": ["src/**/*", "src/node_modules", ".svelte-kit/ambient.d.ts"],
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  }
}
